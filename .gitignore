# VendorConnect .gitignore
# Comprehensive ignore rules for Laravel + Next.js project

# ===========================================
# CRITICAL SECURITY - ENVIRONMENT & CREDENTIALS
# ===========================================
.env
.env.local
.env.production
.env.staging
.env.test
*.env.example

# ===========================================
# DATABASE FILES (CONTAIN SENSITIVE DATA)
# ===========================================
*.sql
*.sql.example
*.sqlite
*.sqlite3
*.db
database_backup_*.sql
demo_data*.sql
real_schema.sql

# ===========================================
# DATABASE MIGRATIONS (CONTAIN SENSITIVE SCHEMA)
# ===========================================
database/migrations/
database/migrations/*.php

# ===========================================
# SENSITIVE BUSINESS LOGIC FILES
# ===========================================
# Permission and admin checking scripts
check_*.php
check-*.sh
clear_*.php
assign_*.php

# Deployment and server management scripts
install_server.sh
deploy_*.sh
rebuild*.sh
protect_*.sh
monitor-*.sh
debug_*.sh
cleanup_*.sh

# Database files (already covered but being explicit)
*.sql
real_schema.sql
database_backup_*.sql
demo_data*.sql

# Load and backup scripts
load_demo_data.sh
backup_database.sh
restore_database.sh

# ===========================================
# SHELL SCRIPTS (CONTAIN CREDENTIALS)
# ===========================================
*.sh
*.sh.example
!vendorconnect-website-clean-header/
!vendorconnect-website-final/

# ===========================================
# OPERATING SYSTEM FILES
# ===========================================
# macOS
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Windows
*.lnk
*.tmp
*.temp
desktop.ini
$RECYCLE.BIN/

# Linux
*~
.fuse_hidden*
.directory
.Trash-*

